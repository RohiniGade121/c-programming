/*ACCEPT INTEGER AND CHECK IT DIVISIBLE BY 3 AND 5*/
#include<stdio.h>
void main()
{
     int n;
      printf("enter integer");
      scanf("%d",&n);
      if(n%3==0&&n%5==0)
       printf("%d integer is divisible by 3 and 5",n);
      else 
       printf("%d integer is not divisible by 3 and 5",n); 
}
