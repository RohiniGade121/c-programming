/* INTERCHANGED NUMBER*/
#include<stdio.h>
 void main()
{
  int a,b,t;
   printf("enter two numbers");
  scanf("%d%d",&a,&b);
  t=a;
 a=b;
 b=t;
  printf("interchanged numbers=%d\t%d",a,b);
}
--------------------------------------------------------------------
/*ACCEPT DIGIT AND PRINT IN WORD*/


#include<stdio.h>
void main()
{
    int n;
    printf("enter digit");
    scanf("%d",&n);
    if(n==1)
     printf("one");
     else if(n==2)
       printf("two");
      else if(n==3)
       printf("three");
      else if(n==4)
       printf("four");
      else if(n==5)
       printf("five");
      else if(n==6)
       printf("six");
      else if(n==7)
       printf("seven");
      else if(n==8)
       printf("eight");
      else if(n==9)
       printf("nine");
      else 
      printf("invalid digit"); 
}
